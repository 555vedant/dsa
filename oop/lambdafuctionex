// Interface with a single abstract method
interface MyFunction {
    int operate(int x, int y);
}

public class LambdaExample {
    public static void main(String[] args) {
        // Using lambda expression to implement the single abstract method of the interface
        MyFunction addition = (a, b) -> a + b;
        System.out.println("Result of addition: " + addition.operate(3, 4));

        MyFunction multiplication = (a, b) -> a * b;
        System.out.println("Result of multiplication: " + multiplication.operate(3, 4));
    }
}
